let sum = fn a b -> a + b;;
let proc = fn f a b -> f a b;;

let test = module () ->
  let test_1st_class_int_sum = proc sum 1 2 == 3;
  let test_1st_class_double_sum = proc sum 1.0 2.0 == 3.0;

  let test_1st_class_binop_int = proc (+) 1 2 == 3;
  let test_1st_class_binop_double = proc (+) 1.0 2.0 == 3.0;
;
