%test

let test_iter_all_void = fn () ->
  let co_void = fn () ->
    yield 1.;
    yield 2.;
    yield 3.
  ;;

  let inst = co_void () in
  (inst () == Some 1.) && (inst () == Some 2.) && (inst () == Some 3.) && (inst () == None)
;;

let test_iter_all_arg = fn () ->
  let co_void = fn a ->
    yield 1.;
    yield a;
    yield 3.
  ;;

  let inst = co_void 2. in
  (inst () == Some 1.) && (inst () == Some 2.) && (inst () == Some 3.) && (inst () == None)
;;

let test_embed_seq = fn () ->

  let ne = fn () ->
    yield 300;
    yield 400
  ;;

  let co_void = fn () ->
    yield 1;
    yield 2;
    yield ne ();
    yield 3
  ;;

  let inst = co_void () in
  (inst () == Some 1) && (inst () == Some 2) && (inst () == Some 300) && (inst () == Some 400) && (inst () == Some 3) && (inst () == None)
;;

let test_rec = fn () ->
  let co_void_rec = fn () ->
    yield 1;
    yield 2;
    yield co_void_rec ()
  ;;

  let inst = co_void_rec () in
  (inst () == Some 1) && (inst () == Some 2) && (inst () == Some 1) && (inst () == Some 2)
;;

