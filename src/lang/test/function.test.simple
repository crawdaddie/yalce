fn func() {
  print(1 + 2)
}

func() # expect: 3

fn void() {
  1 + 2
}

print(void()) # expect: nil

fn nv_func(v) {
  return v + 1
}

print(nv_func(1)) # expect: 2

let a = nv_func

print(a(1)) # expect: 2

fn early_return(v) {
  if (v > 2) {
    return 1
  }
  print("after early return")
  return 0
}


print(early_return(3))
# expect: 1

print(early_return(2))
# expect: after early return
# expect: 0

fn local_vars() {
  let v = 1
  print(1)
}


local_vars() # creates local variables correctly expect: 1
fn local_vars2(v) {
  let v_ = v + 1
  return v_
}

print(local_vars2(3)) # expect: 4

let g = 1
fn mutate(v) {
  v = v + 1
}

mutate(g)
print(g) # variables passed by value expect: 1

